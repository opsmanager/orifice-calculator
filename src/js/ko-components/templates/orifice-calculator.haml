%form
  %ul
    %li
      %label Pipe ID
      %select#pipeID{ "data-bind" => "options: pipeID, value: selectedPipeID" }
    %li
      %label Operating Pressure
      %input#operatingPressure{ :type => "text", "data-bind" => "value: operatingPressure" }
    %li
      %label Operating Pressure Read
      %div{ "data-bind" => "foreach: operatingPressureRead" }
        %div
          %input.operatingPressureRead{ :type => "radio", "data-bind" => "checkedValue: $data, checked: $component.chosenOperatingPressureRead"  }
          %label{ "data-bind" => "text: $data" }
    %li
      %label Operating Pressure Units
      %select#operatingPressureUnits{ "data-bind" => "options: operatingPressureUnits, value: selectedOperatingPressureUnits" }
    %li
      %label Base Specific Gravity
      %input#baseSpecificGravity{ :type => "text", "data-bind" => "value: baseSpecificGravity" }
    %li
      %label Operating Temperature
      %input#operatingTemperature{ :type => "text", "data-bind" => "value: operatingTemperature" }
    %li
      %label Operating Temperature Units
      %div{ "data-bind" => "foreach: operatingTemperatureUnit" }
        %div
          %input.operatingTemperatureUnits{ :type => "radio", "data-bind" => "checkedValue: $data, checked: $component.selectedOperatingTemperatureUnit"  }
          %label{ "data-bind" => "text: $data" }
    %li
      %label Differential Pressure
      %input#differentialPressure{ :type => "text", "data-bind" => "value: differentialPressure" }
    %li
      %label Orifice Bore Diameter
      %input#orificeBoreDiameter{ :type => "text", "data-bind" => "value: orificeBoreDiameter" }
    %li
      %label Compressibility Correction
      %div{ "data-bind" => "foreach: compressibilityCorrection" }
        %div
          %input.compressibilityCorrection{ :type => "radio", "data-bind" => "checkedValue: $data, checked: $component.chosenCompressibilityCorrection"  }
          %label{ "data-bind" => "text: $data" }
